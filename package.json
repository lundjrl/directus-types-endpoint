{
  "name": "directus-extension-types",
  "description": "Endpoint to generate schema types for TS development.",
  "icon": "text_fields",
  "license": "MIT",
  "author": {
    "name": "lundjrl"
  },
  "version": "1.0.1",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-extension-endpoint"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "directus:extension": {
    "type": "endpoint",
    "path": "index.js",
    "source": "src/index.ts",
