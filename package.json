    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "11.0.7",
    "@types/node": "^20.14.7",
    "@types/pluralize": "^0.0.33",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "pluralize": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lundjrl/directus-types-endpoint.git"
  }
}
